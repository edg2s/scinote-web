{
  "env": {
    "browser": true,
    "jquery": true
  },
  "plugins": ["no-jquery"],
  "extends": [
    "airbnb-base/legacy",
    "plugin:no-jquery/deprecated-1.12"
  ],
  "parser": "babel-eslint",
  "parserOptions": {
    "sourceType": "module",
    "ecmaVersion": 5,
    "ecmaFeatures": {
      "impliedStrict": true
    }
  },
  "rules": {
    "space-before-function-paren": ["error", "never"],
    "func-names": ["error", "never"],
    "spaced-comment": [
      "error",
      "always",
      {
        "markers": ["="]
      }
    ],
    "lines-around-comment": [
      "warn",
      {
        "beforeLineComment": false
      }
    ]
  }
}
